{
  "name": "conventional-committer",
  "version": "0.1.1",
  "description": "Commit manager",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "nyc --reporter=text mocha 'dist/test/**/*.js'",
    "build": "tsc",
    "preversion": "npm run test && conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "bin": "./dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wel-shy/committer.git"
  },
  "keywords": [
    "git",
    "tool"
  ],
  "author": "Daniel Welsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wel-shy/committer/issues"
  },
  "homepage": "https://github.com/wel-shy/committer#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chai": "^4.1.7",
    "@types/inquirer": "^6.0.0",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1"
  }
}
